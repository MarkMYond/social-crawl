# Gemma 3n AI Model Configuration
# Copy these to your .env.local file with actual values

# Gemma Azure Container Instance Endpoint
GEMMA_ENDPOINT_URL=http://gemma-3n-e4b-uk.uksouth.azurecontainer.io:8080

# Optional API key for authentication
GEMMA_API_KEY=

# Model configuration
GEMMA_MODEL_SIZE=google/gemma-3n-E4B
GEMMA_MAX_TOKENS=500
GEMMA_TEMPERATURE=0.7
GEMMA_FUNCTION_CALLING=true

