# Azure OpenAI Service
AZURE-OPENAI-ENDPOINT=https://taash-resource.cognitiveservices.azure.com/
AZURE-OPENAI-DEPLOYMENT=gpt-4.1-nano
AZURE-OPENAI-API-VERSION=2025-01-01-preview
AZURE-OPENAI-MODEL-VERSION=2025-04-14
AZURE-OPENAI-TEMPERATURE=0.25

# Development Environment Configuration
NODE-ENV=development

# Server Configuration
PORT=3000
HOST=localhost

# MongoDB Configuration (Development)
# MongoDB-URI is stored in .env.secrets.dashed

# Queue Configuration (names only, connection strings in .env.secrets.dashed)
ASB-POST-OFFICE-QUEUE=post-office
ASB-PREP-MEDIA-QUEUE=prep-media
ASB-AI-SERVICE-QUEUE=ai-service
ASB-CRAWL-MEDIA-QUEUE=media-crawl

# Service Bus Settings
ASB-MAX-CONCURRENT-CALLS=10
ASB-MAX-AUTO-RENEW-DURATION-MINUTES=5

# Throttle Queue Settings
PREP-MEDIA-MAX-CONCURRENT-JOBS=3
AI-SERVICE-MAX-CONCURRENT-JOBS=2

# Logging Configuration
LOG-LEVEL=info
LOG-SERVICE-NAME=social-crawl

# Database Configuration (name only, connection string in secrets)
MONGODB-DATABASE-NAME=social-crawl-dev
